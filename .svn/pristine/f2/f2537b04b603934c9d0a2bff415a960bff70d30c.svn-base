<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:orientation="horizontal"
        android:background="@color/color_primary_blue"
        >
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/x40"
            android:layout_height="match_parent"
            android:text="我的"
            android:gravity="center"
            android:textColor="@color/color_primary_white"
            android:textSize="@dimen/x18"
            />
        <LinearLayout
            android:id="@+id/mine_setting_layout"
            android:layout_width="@dimen/x40"
            android:layout_height="match_parent"
            android:gravity="center"
            >
            <ImageView
                android:layout_marginRight="@dimen/x10"
                android:layout_width="@dimen/x20"
                android:layout_height="@dimen/x20"
                android:src="@mipmap/ic_setting"
                />
        </LinearLayout>
    </LinearLayout>

   <!--<ScrollView-->
       <!--android:layout_width="match_parent"-->
       <!--android:layout_height="match_parent"-->
       <!--android:scrollbars="none"-->
       <!--&gt;-->

       <com.cjj.MaterialRefreshLayout
           android:id="@+id/refresh"
           android:layout_width="match_parent"
           android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/x5"
        android:background="@color/color_primary_white"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x120"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:id="@+id/lin_own"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/setting_userIcon_img"
                    android:scaleType="fitXY"
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/x80"
                    android:layout_marginLeft="@dimen/x15"
                    android:src="@mipmap/icon_default_portrait"
                    />


                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:id="@+id/tv_store_name"
                        android:textColor="@color/color_primary_black"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:layout_marginTop="@dimen/x30"
                        android:textSize="@dimen/x14"
                        android:layout_marginLeft="@dimen/x25"
                        />
                    <LinearLayout
                        android:layout_marginTop="@dimen/x5"
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <!--<ImageView-->
                        <!--android:layout_width="@dimen/x20"-->
                        <!--android:layout_height="@dimen/x20"-->
                        <!--android:layout_marginLeft="@dimen/x15"-->
                        <!--android:src="@mipmap/ic_location"-->
                        <!--/>-->

                        <TextView
                            android:layout_marginLeft="@dimen/x25"
                            android:id="@+id/tv_store_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/color_primary_black"
                            android:textSize="@dimen/x11" />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/lin_store_phone"
                        android:layout_marginTop="@dimen/x5"
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <!--<ImageView-->
                        <!--android:layout_width="@dimen/x20"-->
                        <!--android:layout_height="@dimen/x20"-->
                        <!--android:layout_marginLeft="@dimen/x15"-->
                        <!--android:src="@mipmap/ic_seting_phone"-->
                        <!--/>-->
                        <TextView
                            android:id="@+id/tv_store_tel"
                            android:textColor="@color/text_color"
                            android:layout_marginLeft="@dimen/x25"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/x11"
                            android:text=""
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:gravity="center"
                android:layout_width="@dimen/x60"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/mine_showQr"
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/x30"
                    android:src="@mipmap/ic_qr_code"
                    />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


        <TextView
            android:background="@color/app_fade_gray"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x45"
            android:background="@color/color_primary_white"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="我的余额："
                android:gravity="center_vertical"
                android:textSize="@dimen/x14"
                android:textColor="#2E88E9"
                />
            <TextView
                android:id="@+id/own_tv_money"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="¥0.00"
                android:gravity="center_vertical"
                android:textSize="@dimen/x18"
                android:textColor="#2E88E9"
                />

        </LinearLayout>






    <LinearLayout
        android:layout_marginTop="@dimen/x5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/color_primary_white"
        >

        <TextView
       android:layout_width="@dimen/x300"
       android:layout_height="@dimen/x1"
       android:layout_marginLeft="@dimen/x20"
       android:background="@color/color_fade_gray"
       />

        <LinearLayout
            android:id="@+id/setting_contans_ll"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x45"
            android:background="@color/color_primary_white"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >

            <ImageView
                android:layout_width="@dimen/x25"
                android:layout_height="@dimen/x25"
                android:src="@mipmap/ic_setting_ej"
                android:layout_marginLeft="@dimen/x15"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="联系客服"
                android:gravity="center_vertical"
                android:layout_marginLeft="@dimen/x15"
                android:textSize="@dimen/x14"
                android:textColor="@color/color_primary_black"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="400-100-1234"
                android:textSize="@dimen/x14"
                android:textColor="@color/text_color"
                android:gravity="center_vertical|right"
                />
            <RelativeLayout
                android:layout_width="@dimen/x45"
                android:layout_height="@dimen/x45"
                android:gravity="center"
                >
                <ImageView
                    android:layout_width="@dimen/x20"
                    android:layout_height="@dimen/x20"
                    android:src="@drawable/icon_arrow_gray_right"
                    />
            </RelativeLayout>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:background="@color/color_fade_gray"
            />

        <LinearLayout
            android:visibility="gone"
            android:id="@+id/setting_feedBack_ll"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x46"
            android:background="@color/color_primary_white"
            android:orientation="horizontal"
            android:gravity="center_vertical"
        >
        <ImageView
            android:layout_width="@dimen/x25"
            android:layout_height="@dimen/x25"
            android:src="@mipmap/ic_setting_write"
            android:layout_marginLeft="@dimen/x15"
            />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="使用反馈"
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/x15"
            android:textSize="@dimen/x14"
            android:textColor="@color/color_primary_black"
            />
        <RelativeLayout
            android:layout_width="@dimen/x45"
            android:layout_height="@dimen/x45"
            android:gravity="center"
            >
            <ImageView
                android:layout_width="@dimen/x20"
                android:layout_height="@dimen/x20"
                android:src="@drawable/icon_arrow_gray_right"
                />
        </RelativeLayout>
    </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:background="@color/color_fade_gray"
            />

        <LinearLayout
            android:visibility="gone"
            android:id="@+id/own_agent_mange"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x46"
            android:background="@color/color_primary_white"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <ImageView
                android:layout_width="@dimen/x25"
                android:layout_height="@dimen/x25"
                android:src="@mipmap/ic_agent_mange"
                android:layout_marginLeft="@dimen/x15"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="代理商管理"
                android:gravity="center_vertical"
                android:layout_marginLeft="@dimen/x15"
                android:textSize="@dimen/x14"
                android:textColor="@color/color_primary_black"
                />
            <RelativeLayout
                android:layout_width="@dimen/x45"
                android:layout_height="@dimen/x45"
                android:gravity="center"
                >
                <ImageView
                    android:layout_width="@dimen/x20"
                    android:layout_height="@dimen/x20"
                    android:src="@drawable/icon_arrow_gray_right"
                    />
            </RelativeLayout>
        </LinearLayout>



    </LinearLayout>

    </LinearLayout>
       </com.cjj.MaterialRefreshLayout>
   <!--</ScrollView>-->
</LinearLayout>