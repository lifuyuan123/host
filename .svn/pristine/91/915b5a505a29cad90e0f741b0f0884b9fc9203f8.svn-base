<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    >

    <LinearLayout
        android:layout_width="@dimen/x260"
        android:layout_height="@dimen/x300"
        android:background="@drawable/withdraw_msg_dialog_background"
        android:orientation="vertical"
        android:gravity="center_horizontal">

        <RelativeLayout
            android:layout_width="@dimen/x80"
            android:layout_height="@dimen/x80"
            android:layout_marginTop="@dimen/x20"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/withdraw_msg_Icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_succeed"
                />
        </RelativeLayout>

        <TextView
            android:id="@+id/withdraw_msg_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            android:text="提现成功"
            android:textSize="@dimen/x18"
            android:textStyle="bold"
            android:textColor="@color/color_primary_black"
            />

        <TextView
            android:id="@+id/withdraw_msg_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            android:text="请进入微信查看收款信息"
            android:textSize="@dimen/x13"
            android:textColor="@color/color_primary_black"
            />
            <Button
                android:id="@+id/withdraw_msg_btn"
                android:layout_width="@dimen/x240"
                android:layout_height="@dimen/x40"
                android:layout_marginTop="@dimen/x70"
                android:text="知道了"
                android:textColor="@color/white"
                android:background="@color/color_primary_blue"
                android:textSize="@dimen/x13"
                />

    </LinearLayout>

</LinearLayout>