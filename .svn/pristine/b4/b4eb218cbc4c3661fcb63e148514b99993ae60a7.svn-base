package com.sctjsj.mayk.wowallethost.model.javabean;

import java.io.Serializable;

/**
 * Created by mayikang on 17/5/17.
 */

public class ProjectClassifyBean implements Serializable{

    private int id;
    //是否展示删除的 cb
    private boolean showDelete;
    private int isDelete;
    //是否勾选中 cb
    private boolean isCBChecked;

    public int getIsDelete() {
        return isDelete;
    }

    public void setIsDelete(int isDelete) {
        this.isDelete = isDelete;
    }

    public boolean isCBChecked() {
        return isCBChecked;
    }

    public void setCBChecked(boolean CBChecked) {
        isCBChecked = CBChecked;
    }

    private String name;

    public ProjectClassifyBean() {

    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public boolean isShowDelete() {
        return showDelete;
    }

    public void setShowDelete(boolean showDelete) {
        this.showDelete = showDelete;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "ProjectClassifyBean{" +
                "id=" + id +
                ", showDelete=" + showDelete +
                ", isDelete=" + isDelete +
                ", isCBChecked=" + isCBChecked +
                ", name='" + name + '\'' +
                '}';
    }
}
