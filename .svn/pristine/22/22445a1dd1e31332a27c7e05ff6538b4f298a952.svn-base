<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x45"
        android:orientation="horizontal"
        android:background="@color/white"
        android:gravity="center_vertical"
        >
        <RelativeLayout
            android:id="@+id/agent_parent_choose_layout"
            android:layout_width="@dimen/x40"
            android:layout_height="@dimen/x40"
            android:gravity="center">
            <ImageView
                android:id="@+id/agent_parent_choose"
                android:layout_width="@dimen/x15"
                android:layout_height="@dimen/x15"
                android:src="@drawable/ic_nocheck"
                />
        </RelativeLayout>
        <TextView

            android:id="@+id/agent_parent_tc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x10"
            android:textSize="@dimen/x15"
            android:text="套餐一"
            />

        <RelativeLayout
            android:id="@+id/agent_parent_unfold_layout"
            android:layout_width="@dimen/x40"
            android:layout_height="@dimen/x40"
            android:gravity="center"
            android:layout_marginLeft="@dimen/x180"
            >
            <ImageView
                android:id="@+id/agent_parent_unfold"
                android:layout_width="@dimen/x13"
                android:layout_height="@dimen/x13"
                android:src="@drawable/ico_listdown"
                />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>